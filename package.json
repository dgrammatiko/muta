{
  "name": "@dgrammatiko/muta",
  "version": "2.0.1",
  "joomlaVersion": "4.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Muta",
  "scripts": {
    "get-joomla": "node joomla-tools.mjs -i",
    "link": "joomla-tools -l",
    "build": "joomla-tools -b media_source",
    "release": "joomla-tools -r",
    "init": "joomla-tools -i && joomla-tools -l && joomla-tools -b media_source",
    "lint-css": "npx stylelint ./media_source/.",
    "lint-js": "npx eslint .",
    "lint": "npm run lint-css && npm run lint-js",
    "update:browserlist": "npx browserslist@latest --update-db"
  },
  "author": "Dimitris Grammatikogiannis",
  "license": "MIT",
  "homepage": "https://muta.dgrammatiko.dev",
  "joomla-extensions": {
    "components": [],
    "libraries": [],
    "modules": {
      "administrator": [],
      "site": []
    },
    "plugins": {
      "actionlog": [],
      "api-authentication": [],
      "authentication": [],
      "behaviour": [],
      "captcha": [],
      "content": [],
      "editors": [],
      "editors-xtd": [],
      "extension": [],
      "fields": [],
      "filesystem": [],
      "finder": [],
      "installer": [],
      "media-action": [],
      "multifactorauth": [],
      "privacy": [],
      "quickicon": [],
      "sampledata": [],
      "system": [],
      "task": [],
      "user": [],
      "webservices": [],
      "workflow": []
    },
    "templates": {
      "administrator": [{"name": "muta", "version": "2.0.1"}],
      "site": []
    }
  },
  "devDependencies": {
    "@dgrammatiko/joomla-tools": "0.0.1-alpha.3",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-vue": "9.14.1",
    "stylelint": "15.6.2",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-scss": "5.0.0"
  },
  "dependencies": {
    "@claviska/jquery-minicolors": "^2.3.6",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "awesomplete": "^1.1.5",
    "bootstrap": "5.3.0-alpha3",
    "choices.js": "^10.2.0",
    "color-picker-web-component": "^1.3.7",
    "joomla-ui-custom-elements": "^0.2.0",
    "lit": "2.7.4",
    "shepherd.js": "11.1.1",
    "toolcool-color-picker": "1.0.15"
  }
