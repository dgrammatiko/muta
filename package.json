{
  "name": "@dgrammatiko/muta",
  "version": "2.1.3",
  "joomlaVersion": "4.3.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Muta",
  "scripts": {
    "get-joomla": "joomla-tools.mjs -i",
    "link": "joomla-tools -l",
    "build": "joomla-tools -b media_source",
    "release": "joomla-tools -r",
    "init": "joomla-tools -i && joomla-tools -l && joomla-tools -b media_source",
    "lint-css": "./node_modules/stylelint/bin/stylelint.js ./media_source/**/*.scss --fix",
    "lint-js": "./node_modules/eslint/bin/eslint.js ./media_source/**/*.mjs",
    "lint": "npm run lint-css && npm run lint-js",
    "update:browserlist": "npx browserslist@latest --update-db",
    "update:npm": "npm install $(npm outdated | cut -d' ' -f 1 | sed '1d' | xargs -I '$' echo '$@latest' | xargs echo)  --save-exact"
  },
  "author": "Dimitris Grammatikogiannis",
  "license": "MIT",
  "homepage": "https://muta.dgrammatiko.dev",
  "joomla-extensions": {
    "components": [],
    "libraries": [],
    "modules": {
      "administrator": [],
      "site": []
    },
    "plugins": {
      "actionlog": [],
      "api-authentication": [],
      "authentication": [],
      "behaviour": [],
      "captcha": [],
      "content": [],
      "editors": [],
      "editors-xtd": [],
      "extension": [],
      "fields": [],
      "filesystem": [],
      "finder": [],
      "installer": [],
      "media-action": [],
      "multifactorauth": [],
      "privacy": [],
      "quickicon": [],
      "sampledata": [],
      "system": [],
      "task": [],
      "user": [],
      "webservices": [],
      "workflow": []
    },
    "templates": {
      "administrator": [
        {
          "name": "muta",
          "version": "2.1.3"
        }
      ],
      "site": []
    }
  },
  "devDependencies": {
    "@dgrammatiko/joomla-tools": "github:dgrammatiko/joomla-tools#main"
  },
  "dependencies": {
    "@claviska/jquery-minicolors": "^2.3.6",
    "@fortawesome/fontawesome-free": "6.4.2",
    "awesomplete": "^1.1.5",
    "bootstrap": "5.3.2",
    "choices.js": "^10.2.0",
    "color-picker-web-component": "^1.3.7",
    "joomla-ui-custom-elements": "^0.2.0",
    "lit": "3.0.0",
    "shepherd.js": "11.2.0",
    "toolcool-color-picker": "1.0.15"
  }
}
